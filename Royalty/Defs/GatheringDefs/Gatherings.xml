    <finishedMessage>The concert has concluded.</finishedMessage>
    <duty>Concert</duty>
    <workerClass>GatheringWorker_Concert</workerClass>
    <requiredTitleAny>
      <li>Knight</li>
      <li>Praetor</li>
      <li>Baron</li>
      <li>Count</li>
    </requiredTitleAny>
  </GatheringDef>
  
</Defs>