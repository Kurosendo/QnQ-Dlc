      <suffixSymbols>
        <li><symbol>stick</symbol></li>
        <li><symbol>bone</symbol></li>
        <li><symbol>pole</symbol></li>
        <li><symbol>lank</symbol></li>
        <li><symbol>reed</symbol></li>
        <li><symbol>slink</symbol></li>
        <li><symbol>stilt</symbol></li>
      </suffixSymbols>
    </symbolPack>
