    <displayOrderInCategory>0</displayOrderInCategory>
    <forcedTraits>
      <li>
        <def>PsychicSensitivity</def>
        <degree>-2</degree>
      </li>
    </forcedTraits>
    <biostatMet>2</biostatMet>
    <symbolPack>
      <prefixSymbols>
        <li><symbol>deaf</symbol></li>
