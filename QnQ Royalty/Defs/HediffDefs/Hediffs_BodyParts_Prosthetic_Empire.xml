    <addedPartProps>
      <solid>true</solid>
    </addedPartProps>
    <stages>
      <li>
        <statOffsets>
          <MiningSpeed>1.60</MiningSpeed>
          <DeepDrillingSpeed>0.7</DeepDrillingSpeed>
